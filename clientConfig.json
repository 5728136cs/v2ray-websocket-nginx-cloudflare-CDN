{
  "inbound": {
    "port": 1080,
    "listen": "127.0.0.1",
    "protocol": "socks",
    "domainOverride": ["tls","http"],
    "settings": {
      "auth": "noauth",
      "udp": false
    }
  },
  "outbound": {                             //凡是大写的填写自己的信息
    "protocol": "vmess",
    "settings": {
      "vnext": [
        {
          "address": "YOURDOMAIN.NAME",//填自己的域名，xxx.xx
          "port": 443,
          "users": [
            {
              "id": "填写你自己的UUID",
              "alterId": 64
            }
          ]
        }
      ]
    },
    "streamSettings": {
      "network": "ws",
      "security": "tls",
      "wsSettings": {
        "path": "/tttest" //自定义也可以，保持nginx和服务端客户端一致就行
      }
    }
  },
  "outboundDetour": [
    {
      "protocol": "freedom",
      "settings": {},
      "tag": "direct"
    }
  ],
  "routing": {
    "strategy": "rules",
    "settings": {
      "domainStrategy": "IPOnDemand",
      "rules": [
        {
          "type": "field",
          "outboundTag": "direct",
          "domain": ["geosite:cn"]//这里国内直连
        },
        {
          "type": "chinaip",
          "outboundTag": "direct",
          "ip": ["geoip:cn"]
        }
      ]
    }
  }
  
}
